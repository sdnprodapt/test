{
    "endpoint": "netconf",
    "type": "bpprov.runners.simple.Sequence",
    "tests": [
        "commands/firefly/tests/get-protocols-bgp.json"
    ],
    "endpoint-parameters": {
        "template": "firefly/get-protocols-bgp-command.tmpl"
    },
    "out-schema": "firefly/out-get-protocols-bgp.json",
    "out-path": [
        {
            "type": "bpprov.translators.route.Branch",
            "parameters": {
                "choice": [
                    {
                        "when": "${$..} missing \"name\"",
                        "do": {
                            "type": "bpprov.translators.template.Json",
                            "parameters": {
                                "templateType": "inline",
                                "template": "{\"statusCode\": \"404\", \"reason\": \"Virtual Router name not defined\"}"
                            }
                        }
                    }
                ]
            }
        }
    ],
    "in-path": [
        {
            "type": "bpprov.translators.template.Json",
            "parameters": {
                "template": "firefly/get-protocols-bgp.tmpl"
            }
        }
    ]
}
