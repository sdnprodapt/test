                "<route>",
                "<name>192.168.17.0/24</name>",
                "<next-hop>192.168.16.1000</next-hop>",
                "</route>",
                "</static>",
                "<router-id operation=\"replace\">15.16.17.18</router-id>",
                "<autonomous-system operation=\"replace\">",
                "<as-number>7777</as-number>",
                "</autonomous-system>",
                "<confederation operation=\"replace\">",
                "<confederation-as>64512</confederation-as>",
                "<members>64512</members>",
                "<members>64513</members>",
                "</confederation>",
                "</routing-options>",
                "</configuration>",
                "</config>",
                "</edit-config>",
                "</rpc>"
            ]
        ],
        "in-params": [
            [
                "<rpc-reply>",
                "<ok/>",
                "</rpc-reply>"
            ]
        ]
