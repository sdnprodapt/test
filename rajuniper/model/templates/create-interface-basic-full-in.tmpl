{
{% set props = data['properties'] if 'properties' in data else data%}
{% if 'admin_status' in props %}
    "admin_status": "{{ props['admin_status'] }}",
{% endif %}
{% if 'mtu' in props %}
        "mtu": "{{ props['mtu'] }}",
{% endif %}
{% if 'ipv4_addresses' in props %}
    "ipv4_addresses": [
    {% for address in props['ipv4_addresses'] %}
        "{{ address }}"{% if not loop.last %},{% endif %}
    {% endfor %}
    ],
{% endif %}
{% if 'description' in props %}
    "description": "{{ props.description }}",
{% endif %}
{% if "ccc_filter" in props %}
    "ccc_filter": {
    {% if props["ccc_filter"]["input"] %}
    "input": [
    {% for input_filter in props["ccc_filter"]["input"] %}
    "{{input_filter}}"{% if not loop.last %},{% endif %}
    {% endfor %}
    ],
    {% endif %}
    {% if props["ccc_filter"]["output"] %}
    "output": [
    {% for output_filter in props["ccc_filter"]["output"] %}
    "{{output_filter}}"{% if not loop.last %},{% endif %}
    {% endfor %}
    ]
    {% endif %}
    },
{% endif %}
{% if 'vlan_ids' in props %}
    "vlan_id": "{{ props.vlan_ids[0] }}",
{% endif %}
    "interface": "{{ props.name.split('.')[0] }}",
    "unit": "{{ props.name.split('.')[1] }}"
}
