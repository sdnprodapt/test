				                 <value>CUSTOMERCOMMUNITY</value>
				           </community>
                           <community>
				                 <value>MGMT-Paetec-911</value>
				           </community>
				       </from>
				       <then>
				           <accept/>
				       </then>
				  </term>
				 <term>
			        <term_name>REJECT</term_name>
				     <then>
				           <reject/>
				      </then>
			    </term>
			</policy-statement>
                 <community>
                     <community_name>{{ data.CUSTOMERCOMMUNITY }}</community_name>
                     <members>
                          <value>target:{{ data.ROUTEDISTINGUISHER }}</value>
