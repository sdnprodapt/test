				    <from>
                         <protocol>direct</protocol>
                         <prefix-list-filter>
				             <list_name>MGMT_NETWORKS</list_name>
                             <orlonger></orlonger>
                             <reject></reject>
				         </prefix-list-filter>
                         <prefix-list-filter>
				              <list_name>PAETEC-CPE-MGMT</list_name>
                              <orlonger></orlonger>
				         </prefix-list-filter>
				    </from>
				    <then>
                         <community>
                              <add></add>
					          <community_name>MGMT-Paetec-411</community_name>
				         </community>
				    </then>
				 </term>
				 <term>
			        <term_name>CUST-routes</term_name>
				    <from>
                         <protocol>static</protocol>
