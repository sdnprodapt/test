{% if "ok" in data["output"] %}
{
    "label": "{{ data['properties']['vpn-id'] }}",
    "properties": {{ data["properties"] | to_json }},
    "orchState": "active"
}
{% else %}
{
    "label": "{{ data['properties']['vpn-id'] }}",
    "properties": {{ data["properties"] | to_json }},
    "orchState": "failed",
    "reason": "{{ data['output'] }}"
}
{% endif %}