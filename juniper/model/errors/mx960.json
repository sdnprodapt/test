{
    "patterns": [
        {
            "command": "<get[\\w-]>", 
            "match-reason": "<error-message>(.*?)</error-message>", 
            "onError": [], 
            "pattern": "<error-severity>error</error-severity>"
        }, 
        {
            "command": "<(edit-config|commit/)>", 
            "match-reason": "<error-message>\\s*(.*?)\\s*</error-message>", 
            "onError": [
                {
                    "write": "<rpc><discard-changes/></rpc>"
                }
            ], 
            "pattern": "<error-severity>error</error-severity>"
        }
    ]
}
