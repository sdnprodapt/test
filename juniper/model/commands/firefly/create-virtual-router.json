{
    "endpoint": "null",
    "type": "bpprov.runners.simple.Sequence",
    "tests": [
    ],
    "endpoint-parameters": {
    },
    "out-schema": "firefly/out-create-virtual-router.json",
    "out-path": [
    ],
    "in-path": [{
        "type": "bpprov.translators.aggregator.Call",
        "parameters": {
            "command": "create-routing-options.json",
            "input": {
                "templateType": "file",
                "data": "firefly/create-routing-options-in.tmpl"
            },
            "output": {
                "patches": [{
                    "from": "/data",
                    "to": "/data/routing-options-results"
                }]
            }
        }
    }, {
        "type": "bpprov.translators.route.Branch",
        "parameters": {
            "choice": [{
                "when": "${routing-options-results} contains \"rpc-error\"",
                "do": {
                    "type": "bpprov.translators.aggregator.Call",
                    "parameters": {
                        "command": "discard-changes.json",
                        "output": {
                            "patches": [{
                                "from": "/data",
                                "to": "/data/discard-results"
                            }]
                        }
                    }
                }
            }],
            "otherwise": {
                "type": "bpprov.translators.aggregator.Call",
                "parameters": {
                    "command": "commit.json",
                    "input": {
                        "templateType": "json-dict",
                        "data": {}
                    },
                    "output": {
                        "patches": [{
                            "from": "/data",
                            "to": "/data/commit-results"
                        }]
                    }
                }
            }
        }
    }, {
        "type": "bpprov.translators.route.Branch",
        "parameters": {
            "choice": [{
                "when": "${.} contains \"commit-results\"",
                "do": {
                    "type": "bpprov.translators.route.Branch",
                    "parameters": {
                        "choice": [{
                            "when": "${commit-results} contains \"rpc-error\"",
                            "do": {
                                "type": "bpprov.translators.aggregator.Call",
                                "parameters": {
                                    "command": "discard-changes.json",
                                    "output": {
                                        "patches": [{
                                            "from": "/data",
                                            "to": "/data/discard-results"
                                        }]
                                    }
                                }
                            }
                        }]
                    }
                }
            }]
        }
    }, {
        "type": "bpprov.translators.call.Function",
        "parameters": {
            "function": "juniper.firefly.firefly_errors",
            "args": {
                "error_key": "rpc-error",
                "fields": {
                    "commit": "commit-results",
                    "routing-options": "routing-options-results"
                },
                "errors": ["rpc-error"]
            }
        }
    }]
}